    "cors": "^2.8.5",
    "discord.js": "^14.24.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^164.1.0",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "winston": "^3.11.0"
  },
