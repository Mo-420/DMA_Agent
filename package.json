{
  "name": "dma-agent",
  "version": "1.0.0",
  "description": "AI Agent for DMA document management and yacht availability",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "PORT=${PORT:-3004} nodemon server/index.js",
